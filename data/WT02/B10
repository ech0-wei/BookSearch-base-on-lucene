   for any specific application are given or implied.

   Chris Skinner,  January 30, 1996.
   Michael Moricz, July 4, 1996.
*/

<!-- Beginning of JavaScript Applet -------------------
function rot_banner_indx(seed)
{
        var m1  = "Welcome to F. Chan's homepage on health and nutrition        ";
        var m2  = "    Vitamins and herbs ...    ";
        var m3  = "    Safe, effective, no side effects, all natural ...     ";
        var m4  = "    Guaranteed to work!       ";
        var m5  = "Vitamins and herbs, vitamins and herbs, vitamins and herbs...   ";
        
        var msg=m1+m2+m3+m4+m5;
        var out = " ";
        var c   = 1;

        if (seed > 100) {
                seed--;
                var cmd="rot_banner_indx(" + seed + ")";
                timerTwo=window.setTimeout(cmd,100);
        }
        else if (seed <= 100 && seed > 0) {
                for (c=0 ; c < seed ; c++) {
                        out+=" ";
                }
                out+=msg;
                seed--;
                var cmd="rot_banner_indx(" + seed + ")";
                    window.status=out;
                timerTwo=window.setTimeout(cmd,100);
        }
        else if (seed <= 0) {
                if (-seed < msg.length) {
                        out+=msg.substring(-seed,msg.length);
                        seed--;
                        var cmd="rot_banner_indx(" + seed + ")";
                        window.status=out;
                        timerTwo=window.setTimeout(cmd,100);
                }
                else {
                        window.status=" ";
                        timerTwo=window.setTimeout("rot_banner_indx(100)",75);
                }
        }
}
// -- End of JavaScript code -------------- -->
</SCRIPT>

</HEAD>

