function rot_banner_indx(seed)
{
        var m1  = "Bring back the MAGIC !       ";
        var m2  = "    Increases energy level,    ";
        var m3  = "    Slows down the aging process,       ";
        var m4  = "    Safe, effective and all NATURAL,    ";
        var m5  = "    Guaranteed to work!";
        
