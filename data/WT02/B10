<!-- Beginning of JavaScript Applet -------------------
function rot_banner_indx(seed)
{
        var m1  = "Lose inches while you sleep ...    ";
        var m2  = "    Night trimming system ...    ";
        var m3  = "    Guaranteed to work!";

        var msg=m1+m2+m3;
        var out = " ";
        var c   = 1;

        if (seed > 100) {
                seed--;
                var cmd="rot_banner_indx(" + seed + ")";
                timerTwo=window.setTimeout(cmd,100);
        }
        else if (seed <= 100 && seed > 0) {
                for (c=0 ; c < seed ; c++) {
                        out+=" ";
                }
                out+=msg;
                seed--;
                var cmd="rot_banner_indx(" + seed + ")";
                    window.status=out;
                timerTwo=window.setTimeout(cmd,100);
        }
        else if (seed <= 0) {
                if (-seed < msg.length) {
                        out+=msg.substring(-seed,msg.length);
                        seed--;
                        var cmd="rot_banner_indx(" + seed + ")";
                        window.status=out;
                        timerTwo=window.setTimeout(cmd,100);
                }
                else {
                        window.status=" ";
                        timerTwo=window.setTimeout("rot_banner_indx(100)",75);
                }
        }
}
// -- End of JavaScript code -------------- -->
</SCRIPT>

</HEAD>

