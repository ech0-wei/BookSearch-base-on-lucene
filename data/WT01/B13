<HTML>
<HEAD>
<TITLE>How The Markets React</TITLE>
<SCRIPT LANGUAGE="JavaScript">

<!--  
var newwin = null;
var again = 1;
function show(url,x,y) {
        winfeatures = "width=" + x + ",height=" + y + ",scrollbars=yes";
        newwin = window.open(url, "newwin_name", winfeatures);
        if (newwin != null) {
                if (newwin.opener == null) {
                        newwin.opener = self;
                }
                newwin.location.href = url;
        }
        if (again == 1) {
                again = 0;
                show(url,x,y);
        }
        else {
                again = 1;
        }
}
