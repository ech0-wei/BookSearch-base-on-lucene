  <TABLE BORDER=0 CELLSPACING=0 CELLPADDING=0>
  <TR>
  <TD VALIGN=TOP COLSPAN=2>
  <A HREF="../menu.html">
  <IMG BORDER=0 SRC="../gifs/icons/kemper_blkwht.gif" ALT="Kemper Funds" HEIGHT=62 WIDTH=202></A></TD>
  </TR>
  <TR>
  <TD VALIGN=TOP>
	<TABLE BORDER=0 CELLSPACING=0 CELLPADDING=0>
	<TR>
	<TD VALIGN=TOP NOWRAP WIDTH=45></TD>
